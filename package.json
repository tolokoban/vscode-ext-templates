{
    "name": "tolokoban-templates",
    "displayName": "tolokoban-templates",
    "description": "Quick customizable templates available in the file epxlorer",
    "version": "0.4.1",
    "publisher": "tolokoban",
    "icon": "logo.jpg",
    "engines": {
        "vscode": "^1.84.2"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "tolokoban-templates.applyTemplate",
                "title": "Tolokoban: Apply Template"
            },
            {
                "command": "tolokoban-templates.editTemplates",
                "title": "Tolokoban: Edit Templates"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "explorerResourceIsFolder",
                    "command": "tolokoban-templates.applyTemplate",
                    "group": "YourGroup@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production --devtool hidden-source-map",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "vsce package",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@tolokoban/type-guards": "^0.5.0",
        "@types/glob": "^8.1.0",
        "@types/js-yaml": "^4.0.9",
        "@types/json5": "^2.2.0",
        "@types/mocha": "^10.0.4",
        "@types/node": "20.9.0",
        "@types/vscode": "^1.84.1",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "@vscode/test-electron": "^2.3.6",
        "@vscode/vsce": "^2.22.0",
        "eslint": "^8.53.0",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "ts-loader": "^9.5.0",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "js-yaml": "^4.1.0",
        "json5": "^2.2.3"
    }
}